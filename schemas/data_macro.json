{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "The song data in keyboard macro format.",
    "type": "object",
    "properties": {
        "events": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "enum": [ 1, 2 ]
                    },
                    "value": {
                        "type": "number"
                    }
                }
            }
        }
    },
    "required": [ "events" ]
}